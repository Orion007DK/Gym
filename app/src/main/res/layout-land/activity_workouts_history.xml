    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activites.workoutsHistory.WorkoutsHistoryActivity">

    <fragment
        android:id="@+id/workoutsHistoryListFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        class="com.example.gym.activites.workoutsHistory.WorkoutsHistoryListFragment"
