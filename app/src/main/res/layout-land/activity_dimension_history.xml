<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activites.dimensionHistory.DimensionHistoryActivity">
    
    <fragment
        android:id="@+id/dimensionsHistoryListFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        class="com.example.gym.activites.dimensionHistory.DimensionHistoryListFragment"
        android:layout_weight="1"
        >
    </fragment>
    <fragment
        android:id="@+id/dimensionsHistoryDetailFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        class="com.example.gym.activites.dimensionHistory.DimensionsHistoryDetailFragment"
        android:layout_weight="1"
        >
    </fragment>

</androidx.appcompat.widget.LinearLayoutCompat>